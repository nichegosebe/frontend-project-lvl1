{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "airbnb", "airbnb/hooks", "plugin:lodash/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["lodash"],
  "rules": {
    "no-console": "off",
    "import/extensions": "off"
  }
}
